<% layout('/layouts/boilerplate') -%>
<body>
    <ul>
        <li><%= listing.title  %></li>
        <li><%= listing.description  %></li>
        <li>Rs. <%= listing.price.toLocaleString("en-NP")  %></li>
        <li><%= listing.location  %></li>
        <li><%= listing.country  %></li>
    </ul>
    <button><a href="/listings/<%= listing.id %>/edit">Edit</a></button>
    <form action="/listings/<%= listing.id %>/delete?_method=DELETE" method="post">
        <button>Delete This Listing</button>
    </form>
</body>
